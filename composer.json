{
    "name": "mbezhanov/silex-rest-api-example",
    "keywords": ["silex", "rest", "api", "example"],
    "description": "An example REST API implemented in Silex",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marin Bezhanov",
            "email": "marin.bezhanov@gmail.com",
            "homepage": "http://marinbezhanov.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/App"
        }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests" }
    },
    "minimum-stability": "RC",
    "require": {
        "php": "^7.1",
        "silex/silex": "^2.1",
        "dflydev/doctrine-orm-service-provider": "^2.0",
        "jdesrosiers/silex-cors-provider": "~1.0",
        "kurl/silex-doctrine-migrations-provider": "^0.3",
        "lcobucci/jwt": "^3.2",
        "mbezhanov/silex-route-annotations": "^0.2.1",
        "pagerfanta/pagerfanta": "^1.0",
        "symfony/cache": "^3.3",
        "symfony/console": "^3.3",
        "symfony/dotenv": "^3.3",
        "symfony/filesystem": "^3.3",
        "symfony/validator": "^3.3",
        "willdurand/hateoas": "^2.11"
    },
    "require-dev": {
        "mbezhanov/silex-alice-data-fixtures": "^0.2",
        "mbezhanov/faker-provider-collection": "^0.1",
        "php-mock/php-mock": "^2.0",
        "phpunit/phpunit": "^6.3"
    }
}
